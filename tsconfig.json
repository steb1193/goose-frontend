{
  "compilerOptions": {
    "target": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "types": ["vite/client"],
    "module": "esnext",
    "moduleResolution": "bundler"
  },
  "include": ["src", "vite.config.ts"]
}

